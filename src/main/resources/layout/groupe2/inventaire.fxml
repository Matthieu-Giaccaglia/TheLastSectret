<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="127.0" maxWidth="520.0" minHeight="22.0" minWidth="320.0" prefHeight="64.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.umontpellier.iut.groupe2.inventaire.InventaireController">
    <GridPane fx:id="inventoryGrid" maxHeight="64.0" maxWidth="320.0" minHeight="64.0" minWidth="320.0" prefHeight="64.0" prefWidth="320.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <ImageView fx:id="slotUn" fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="0">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fx:id="slotDeux" fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fx:id="slotTrois" fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fx:id="slotQuatre" fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fx:id="slotCinq" fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="4">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fx:id="slotItemUn" fitHeight="60.0" fitWidth="60.0" nodeOrientation="INHERIT" onMouseClicked="#changeImage" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="slotItemDeux" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" />
      <ImageView fx:id="slotItemTrois" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" />
      <ImageView fx:id="slotItemQuatre" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" />
      <ImageView fx:id="slotItemCinq" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="4" />
    </GridPane>

</AnchorPane>
