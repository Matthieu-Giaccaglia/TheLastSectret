        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <ImageView fx:id="slotUn" fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="0">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
        <ImageView fitHeight="64" fitWidth="64" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="4">
            <Image url="@../../raw/groupe1/inventory_slot.jpg" />
        </ImageView>
    </GridPane>

</AnchorPane>
